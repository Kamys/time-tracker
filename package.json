    "electron": "^3.0.10",
    "electron-load-devtool": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "img-loader": "^3.0.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "typescript": "^3.2.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "electron-builder": "^20.38.2"
  },
  "dependencies": {
    "active-win": "^4.0.0",
