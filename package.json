{
  "name": "timetracker",
  "version": "1.0.0",
  "description": "",
  "main" : "./main.js",
  "scripts": {
    "dev": "webpack-dev-server --mode=development --env.render",
    "build-electron": "webpack --mode=development --env.main;",
    "dev-electron": "npm run build-electron; npx electron ./build/main.js",
    "build": "npm run clear; NODE_ENV=production webpack --mode=production",
    "build-electron-app": "npm run build; cd build; npx electron-builder -wlm",
    "build-prod-linux": "npm run build; cd build; npx electron-builder --linux deb tar.xz",
    "clear": "rm -rf build;"
  },
  "license": "ISC",
